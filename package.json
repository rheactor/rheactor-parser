{
  "name": "@rheactor/rheactor-parser",
  "description": "Rheactor Parser allows to perform text parsing.",
  "version": "0.1.0",
  "license": "Apache License 2.0",
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "scripts": {
    "dev": "tsc --watch",
    "build": "rimraf dist/ && tsc -p tsconfig.prod.json && tsc-alias && swc ./dist -d ./dist",
    "prettier": "prettier --check ./src",
    "prettier:fix": "prettier --write ./src",
    "eslint": "eslint .",
    "eslint:fix": "eslint --fix .",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "test:coverage": "jest --coverage --watchAll",
    "test:coverage-strict": "jest --collect-coverage"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@swc/cli": "^0.1.59",
    "@swc/core": "^1.3.27",
    "@types/jest": "^29.2.6",
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "@typescript-eslint/parser": "^5.48.2",
    "eslint": "^8.32.0",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "jest": "^29.3.1",
    "prettier": "^2.8.3",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.2",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "rimraf": "^4.1.1"
  }
}
